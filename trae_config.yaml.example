agents:
    trae_agent:
        enable_lakeview: true
        model: trae_agent_model
        max_steps: 200
        tools:
            - bash
            - str_replace_based_edit_tool
            - sequentialthinking
            - task_done
allow_mcp_servers:
    - playwright
mcp_servers:
    playwright:
        command: npx
        args:
            - "@playwright/mcp@0.0.27"
    # Optional timeout in seconds to avoid hangs during initialization
    timeout: 15
lakeview:
    model: lakeview_model

model_providers:
    anthropic:
        api_key: your_anthropic_api_key
        provider: anthropic
    pollinations:
        # OpenAI-compatible configuration for Pollinations.ai
        # You can also set these via environment variables:
        #   POLLINATIONS_API_KEY and POLLINATIONS_BASE_URL
        api_key: your_pollinations_api_key
        base_url: http://text.pollinations.ai/openai

models:
    trae_agent_model:
        model_provider: anthropic
        model: claude-4-sonnet
        max_tokens: 4096
        temperature: 0.5
        top_p: 1
        top_k: 0
        max_retries: 10
        parallel_tool_calls: true
    lakeview_model:
        model_provider: anthropic
        model: claude-3.5-sonnet
        max_tokens: 4096
        temperature: 0.5
        top_p: 1
        top_k: 0
        max_retries: 10
        parallel_tool_calls: true
    pollinations_default:
        model_provider: pollinations
        model: deepseek-reasoning
        max_tokens: 2048
        temperature: 0.5
        top_p: 1
        max_retries: 5
